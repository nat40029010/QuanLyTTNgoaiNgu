@model IEnumerable<QuanLyTTNgoaiNgu.Models.KETQUAHOCTAP>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
