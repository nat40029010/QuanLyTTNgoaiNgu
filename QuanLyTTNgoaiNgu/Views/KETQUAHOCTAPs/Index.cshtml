                @Html.DisplayNameFor(model => model.Diem)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ChuyenCan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HOCVIEN)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LOPHOC)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Diem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChuyenCan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HOCVIEN.DiaChi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LOPHOC.TenLop)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaKetQua">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaKetQua">Details</a> |
